#popup_ok {
	border: 1px solid #dbe6ed;
	border-radius: 5px;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	width: 80px;
	height: 25px;
	font-family: "Microsoft YaHei";
	background: #FFF;
	color: #000;
	font-size: 13px;
	margin-left: -60px;
	float: right
}

#popup_ok:hover {
	color: #FFF;
	background: #00decf;
	cursor: pointer;
	border: 1px solid #00decf;
}

#popup_cancel {
	border: 1px solid #dbe6ed;
	border-radius: 5px;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	width: 80px;
	height: 25px;
	font-family: "Microsoft YaHei";
	background: #FFF;
	color: #000;
	font-size: 13px;
	/*//    margin-left: -60px;*/
	margin-right: 75px;
	float: right;
}

#popup_cancel:hover {
	color: #FFF;
	background: #00decf;
	cursor: pointer;
	border: 1px solid #00decf;
}

.favorite {
	width: 460px !important;
	height: 215px !important;
	margin: 125px auto !important;
	position: fixed;
	background-color: white;
	z-index: 99999;
	left: 0 !important;
	right: 0 !important;
	box-shadow: 0px 1px 13px 0px #b3bbc2 !important;
	padding: 25px !important;
	box-sizing: border-box !important
}

div.favorite span {
	font-size: 14px;
	color: #7e8a93;
}

.favorite h1 {
	display: none
}

div.favorite span.favorite_content {
	display: block;
	padding: 50px 0;
	text-align: center;
	font-size: 15px;
	vertical-align: top;
	line-height: 20px
}

div.favorite button {
	float: right;
	margin-right: 10px
}