.r-window-mask{z-index:1000;position: fixed;top:0;left:0;width:100%; height:100%;background:url(../images/window/r-window-mask.png) repeat;}      
.r-window-shadow{width:100%;height:100%;-webkit-box-shadow: #888 0px 0px 6px;-moz-box-shadow: #888 0px 0px 6px;-ms-box-shadow: #888 0px 0px 6px;-o-box-shadow: #888 0px 0px 6px;box-shadow: #888 0px 0px 6px;}    
.r-window{z-index:1000;position:fixed;left:50%;left:50%;cursor: default;}
.r-window.vhide{visibility: hidden;}
.r-window-main{position:absolute;top:0;left:0;width:100%;height:100%;overflow: hidden;background-color: #fff;}
.r-window-radius{-webkit-border-radius: 16px;-moz-border-radius: 16px;-ms-border-radius: 16px;-o-border-radius: 16px;border-radius: 16px;} 
.r-window-title{height:30px;line-height:30px;padding:0px 10px;background:url(../images/window/windowtitle_1.png) no-repeat;-webkit-border-radius:5px 5px 0 0;-moz-border-radius:5px 5px 0 0;-ms-border-radius:5px 5px 0 0;-o-border-radius:5px 5px 0 0;border-radius:5px 5px 0 0; color:#fff;}
#deleteTips .r-window-title{background: url(../images/alert_editheader.png) no-repeat;}
.r-window-cnt{overflow:auto;}
.r-window-buttonwrap{/*position:absolute;bottom:0;left:0;width:100%;height:40px;*/height:52px; padding-top:12px; text-align:right;}
.r-window-button{display:inline-block;margin:0px 23px 0px 0px;
  -webkit-transition: background-color,border-radius .3s ease 0s;
  -moz-transition: background-color,border-radius .3s ease 0s;
  -ms-transition: background-color,border-radius .3s ease 0s;
  -o-transition: background-color,border-radius .3s ease 0s;
  transition: background-color,border-radius .3s ease 0s;
}
.r-window-button:hover{
  -webkit-border-radius: 15px;
  -moz-border-radius: 15px;
  -ms-border-radius: 15px;
  -o-border-radius: 15px;
  border-radius: 15px;
}
.r-window-resize-n,.r-window-resize-e,.r-window-resize-s,.r-window-resize-w{z-index:1;position:absolute;font-size:0;background-color: transparent;}
.r-window-resize-n{left:0;top:0px;width:100%;height:5px;cursor: n-resize;}
.r-window-resize-e{right:0px;top:0;width: 5px;height: 100%;cursor: e-resize;}
.r-window-resize-s{left:0;bottom:0px;width:100%;height:5px;cursor: s-resize;}
.r-window-resize-w{left:0px;top:0;width: 5px;height: 100%;cursor: w-resize;}
.r-window-resize-ne,.r-window-resize-se,.r-window-resize-sw,.r-window-resize-nw{z-index:1;position:absolute;width:5px;height: 5px;font-size:0;background-color: transparent;}
.r-window-resize-ne{right: 0px;top: 0px;cursor: ne-resize;}
.r-window-resize-se{right: 0px;bottom: 0px;cursor: se-resize;}
.r-window-resize-sw{left: 0px;bottom: 0px;cursor: sw-resize;}
.r-window-resize-nw{left: 0px;top: 0px;cursor: nw-resize;}
.r-window-title-oplist{padding-top:5px;}
.r-window-title-up,.r-window-title-down,.r-window-title-min,.r-window-title-max,.r-window-title-revert,.r-window-title-close{width: 21px;height: 19px;margin-left: 5px;background:url(../images/window/tool.png) no-repeat;}
.r-window-title-up{background-position: -170px 0;}
.r-window-title-up:hover{background-position: -170px -29px;}
.r-window-title-down{display:none;background-position: -199px 0;}
.r-window-title-down:hover{background-position: -199px -29px;}
.r-window-title-min {background-position: -29px 0;}
.r-window-title-min:hover {background-position: -29px -29px;}
.r-window-title-max {background-position: -60px 0;}
.r-window-title-max:hover {background-position: -60px -29px;}
.r-window-title-revert {display:none;background-position: -149px 0;}
.r-window-title-revert:hover {background-position: -149px -29px;}
/*关闭按钮*/
.r-window-title-close{position:relative;width:19px;height:19px;background: none;}
.r-window-title-close span{display:block; display:none\9; width:19px;height:19px;background:url(../images/window/r-window-title-close.png) center -57px no-repeat;
 -webkit-transform: scale(0);
  -moz-transform: scale(0);
  -ms-transform: scale(0);
  -o-transform: scale(0);
  transform: scale(0);
  -webkit-transition: all 0.2s;
  -moz-transition: all 0.2s;
  -ms-transition: all 0.2s;
  -o-transition: all 0.2s;
  transition: all 0.2s;
}
.r-window-title-close em{position: absolute;left:0;top:0;width:19px;height:19px;background:url(../images/window/r-window-title-close.png) center 0px no-repeat;}
.r-window-title-close:hover span{
	display:block\9;
  -webkit-transform: scale(1);
  -moz-transform: scale(1);
  -ms-transform: scale(1);
  -o-transform: scale(1);
  transform: scale(1);
}
.r-window-title-close:hover em{background-position: center -38px;}

.materialPopIcon{width:12px;height:12px;margin-top:10px; margin-right:12px;background:url(../images/window/windowicon_1.png) 0 center no-repeat ;}
.r-window-buttonwrap .accountpublish{line-height:30px; padding-left:6px; text-align:center;}
.r-window-buttonwrap .accountcancel{line-height:30px; padding-left:6px; text-align:center;}

