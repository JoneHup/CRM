@charset "utf-8";

.r-combobox-box{position:relative; background:url(../images/combobox/comboboxbg.png) no-repeat;}
.r-combobox-box:hover{background: url(../images/blueInput_bg.png) no-repeat;}
.r-combobox-input{width:209px; height:30px; line-height:30px; padding-left:5px; padding-right:33px; border:none; background:none;}
/*.r-combobox-input:hover{border:none; background:none;
    -webkit-box-shadow: 0 0 4px rgba(106, 161, 255, 0.4);
    -moz-box-shadow: 0 0 4px rgba(106, 161, 255, 0.4);
    -ms-box-shadow: 0 0 4px rgba(106, 161, 255, 0.4);
    -o-box-shadow: 0 0 4px rgba(106, 161, 255, 0.4);
    box-shadow: 0 0 4px rgba(106, 161, 255, 0.4);
}*/
/*.r-combobox-input:hover + .r-combobox-handle{background:url(../images/combobox/arrow_down_1.png) no-repeat center center;}*/
.r-combobox-handle{position:absolute; top:1px; right:0px; width:30px; height:30px; background:url(../images/combobox/arrow_down.png) no-repeat center center;}
.r-combobox-handle-up{background:url(../images/combobox/arrow_up.png) no-repeat center center;}
.r-combobox-slider{position:absolute; top:30px; width:254px; padding-bottom:10px; background:url(../images/combobox/comboboxdlbg.png) no-repeat bottom center; border:none; z-index:996; overflow:auto;}
.r-combobox-slider dd{height:26px; line-height:26px; margin:0px 2px; padding:0px 10px; /*border-bottom:1px solid #254a99;*/ cursor:pointer;}
.r-combobox-slider dd span{width:90%; color:#333; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
.r-combobox-now{border:none; background:url(../images/combobox/comboboxsltbg.png) no-repeat;}
/*.r-combobox-now .r-combobox-input + .r-combobox-handle{background:url(../images/combobox/arrow_down_1.png) no-repeat center center;}*/
.r-combobox-hover{/*background:url("../images/combobox/ddsltbg.png") repeat*/ background-color:#dbdbdb;}
.r-combobox-slt{/*background:url("../images/combobox/ddsltbg.png") repeat;*/ background-color:#dbdbdb;}
.r-combobox-chk{margin:6px 5px 0px 0px;}
.r-combobox-clear{display:none; position:absolute; top:1px; right:25px; width:10px; height:24px; background:url(../images/combobox/comboboxclear.png) no-repeat center center;}
.r-combobox-clear-box .r-combobox-clear{display:block;}
.r-combobox-loadding{height:60px; background:url(../images/combobox/loadding.gif) no-repeat center center;}
.r-combobox-error{padding:10px 0; text-align:center; background:#e6e6e6; color:#666;}
.r-combobox-serviceError{display:inline-block; width:100%; height:30px; background:url(../images/combobox/serviceError.png) no-repeat center center;}
.r-combobox-sendError{display:inline-block; width:100%; height:30px; background:url(../images/combobox/sendError.png) no-repeat center center;}
.r-combobox-noData{display:inline-block; width:100%; height:30px; background:url(../images/combobox/noData.png) no-repeat center center;}

#combInGrid_1,#combInGrid_2,#combInGrid_3,#combInGrid_4{margin:4px 0px 0px 12px;}
#combInGrid_1 .r-combobox-slider{position:fixed;}
#combInGrid_2 .r-combobox-slider{position:fixed;}
#combInGrid_3 .r-combobox-slider{position:fixed;}
#combInGrid_4 .r-combobox-slider{position:fixed;}
.siftMoreBox .r-combobox-box{margin:9px 20px 0px 0px;}

#materialStoreComb_2 .r-combobox-box,#materialStoreComb_3 .r-combobox-box{background:url(../images/combobox/comboboxbg_small.png) no-repeat;}
#materialStoreComb_2 .r-combobox-now,#materialStoreComb_3 .r-combobox-now{background:url(../images/combobox/comboboxsltbg_small.png) no-repeat;}
#materialStoreComb_2 .r-combobox-slider,#materialStoreComb_3 .r-combobox-slider{background:url(../images/combobox/comboboxdlbg_small.png) no-repeat bottom center;}

.smallComb .r-combobox-box{background:url(../images/combobox/comboboxbg_small.png) no-repeat;}
.smallComb .r-combobox-now{background:url(../images/combobox/comboboxsltbg_small.png) no-repeat;}
.smallComb .r-combobox-slider{background:url(../images/combobox/comboboxdlbg_small.png) no-repeat bottom center;}

.organizationLikeComb .r-combobox-box{width:140px;}
.organizationLikeComb .r-combobox-input{width: 110px; padding-right: 23px;}
.organizationLikeComb .r-combobox-slider{width: 140px; max-height: 150px;}
.organizationLikeComb .r-combobox-slider dd{padding-left:28px;}
.likeCombIcon_1{background:url(../images/likecombicon_1.png) no-repeat 5px center;}
.likeCombIcon_2{background:url(../images/likecombicon_2.png) no-repeat 5px center;}
.likeCombIcon_1:hover{background-color:#dbdbdb;}
.likeCombIcon_2:hover{background-color:#dbdbdb;}






























